{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/",
        "destination": "/wdc/wdc.html"
      },
      {
        "source": "/wdc",
        "destination": "/wdc/index.html"
      },
      {
        "source": "/muc",
        "destination": "/muc/index.html"
      },
      {
        "source": "/galeri",
        "destination": "/galeri/src/index.html"
      },
      {
        "source": "/ctf-soon",
        "destination": "/ctf-soon/index.html"
      },
      {
        "source": "/semnas-soon",
        "destination": "/semnas-soon/index.html"
      },
      {
        "source": "/sponsor-soon",
        "destination": "/sponsor-soon/index.html"
      },
      {
        "source": "/store-soon",
        "destination": "/store-soon/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/ctf",
        "destination": "/ctf-soon",
        "type": 301
      },
      {
        "source": "/semnas",
        "destination": "/semnas-soon",
        "type": 301
      },
      {
        "source": "/sponsor",
        "destination": "/sponsor-soon",
        "type": 301
      },
      {
        "source": "/store",
        "destination": "/store-soon",
        "type": 301
      }
    ]
  }
}
