{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/",
        "destination": "/wdc/wdc.html"
      },
      {
        "source": "/wdc",
        "destination": "/wdc/index.html"
      }
    ],
    "errorPage": "/404/src/index.html"
  }
}
